@using PartDesk.Web.Classes.Ext
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title | PartDesk - стол заказа автозапчастей</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="Yuri Korshev, SoftGears LLC" />
    <meta name="description" content="PartDesk - уникальный стол для заказа автозапчастей и обработки заказов" />
    <meta name="keywords" content="автозапчасти, авто, заказ, логистика" />
    <meta name="application-name" content="PartDesk" />

    <!-- Headings -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,800,700' rel='stylesheet' type='text/css'>
    <!-- Text -->
    <link href='http://fonts.googleapis.com/css?family=Droid+Sans:400,700' rel='stylesheet' type='text/css' />

    <!--[if lt IE 9]>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400" rel="stylesheet" type="text/css" />
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:700" rel="stylesheet" type="text/css" />
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:800" rel="stylesheet" type="text/css" />
    <link href="http://fonts.googleapis.com/css?family=Droid+Sans:400" rel="stylesheet" type="text/css" />
    <link href="http://fonts.googleapis.com/css?family=Droid+Sans:700" rel="stylesheet" type="text/css" />
    <![endif]-->

    <!-- Core stylesheets do not remove -->
    <link href="~/Content/genyx/css/bootstrap/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/genyx/css/bootstrap/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/genyx/css/icons.css" rel="stylesheet" />

    <!-- Plugins stylesheets -->
    <link href="~/Content/genyx/js/plugins/misc/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <link href="~/Content/genyx/js/plugins/forms/uniform/uniform.default.css" rel="stylesheet" />
    <link href="~/Content/genyx/js/plugins/ui/jgrowl/jquery.jgrowl.css" rel="stylesheet" />
    <link href="~/Content/genyx/js/plugins/forms/select2/select2.css" rel="stylesheet" />

    <!-- app stylesheets -->
    <link href="~/Content/genyx/css/app.css" rel="stylesheet" />

    <!-- Custom stylesheets ( Put your own changes here ) -->
    <link href="~/Content/genyx/css/custom.css" rel="stylesheet" />

    <!--[if IE 8]><link href="~/Content/genyx/css/ie8.css" rel="stylesheet" type="text/css" /><![endif]-->

    <!-- Force IE9 to render in normal mode -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="~/Content/genyx/js/html5shiv.js"></script>
      <script src="~/Content/genyx/js/respond.min.js"></script>
    <![endif]-->

    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/genyx/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/genyx/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/genyx/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/genyx/images/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="~/Content/genyx/images/ico/favicon.png">
    <!-- Le javascript
    ================================================== -->
    <!-- Important plugins put in all pages -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.2/jquery-ui.min.js"></script>
    <script src="~/Content/genyx/js/conditionizr.min.js"></script>
    <script src="~/Content/genyx/js/bootstrap/bootstrap.js"></script>
    <script src="~/Content/genyx/js/plugins/core/nicescroll/jquery.nicescroll.min.js"></script>
    <script src="~/Content/genyx/js/plugins/core/jrespond/jRespond.min.js"></script>
    <script src="~/Content/genyx/js/jquery.genyxAdmin.js"></script>

    <!-- Charts plugins -->
    <script src="~/Content/genyx/js/plugins/charts/flot/jquery.flot.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/flot/jquery.flot.pie.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/flot/jquery.flot.resize.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/flot/jquery.flot.tooltip.min.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/flot/jquery.flot.orderBars.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/flot/jquery.flot.time.min.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/sparklines/jquery.sparkline.min.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/flot/date.js"></script>
    <!-- Only for generating random data delete in production site-->
    <script src="~/Content/genyx/js/plugins/charts/pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/gauge/justgage.1.0.1.min.js"></script>
    <script src="~/Content/genyx/js/plugins/charts/gauge/raphael.2.1.0.min.js"></script>

    <!-- Form plugins -->
    <script src="~/Content/genyx/js/plugins/forms/uniform/jquery.uniform.min.js"></script>
    <script src="~/Content/genyx/js/plugins/forms/mask/jquery.mask.min.js"></script>
    <script src="~/Content/genyx/js/plugins/forms/validation/jquery.validate.js"></script>
    <script src="~/Content/genyx/js/plugins/forms/select2/select2.js"></script>

    <!-- Misc plugins -->
    <script src="~/Content/genyx/js/plugins/misc/fullcalendar/fullcalendar.min.js"></script>

    <!-- UI plugins -->
    <script src="~/Content/genyx/js/plugins/ui/jgrowl/jquery.jgrowl.min.js"></script>

    <!-- Init plugins -->
    <script src="~/Content/genyx/js/app.js"></script>
    <!-- Core js functions -->
    <script src="~/Content/genyx/js/pages/dashboard.js"></script>
    <!-- Init plugins only for page -->


    @Html.Partial("UINotification")
</head>
<body>
    <header id="header">
        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <a class="navbar-brand" href="/dashboard">
                <img src="~/Content/images/logo-small.png" alt="PartDesk" class="img-responsive"></a>
            <button type="button" class="navbar-toggle btn-danger" data-toggle="collapse" data-target="#navbar-to-collapse">
                <span class="sr-only">Переключить левое меню</span>
                <i class="icon16 i-arrow-8"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbar-to-collapse">
                <form id="top-search" class="navbar-form navbar-left" role="search">
                    <div class="input-group">
                        <input type="text" name="tsearch" id="tsearch" placeholder="Введите артикул детали" class="search-query form-control">
                        <span class="input-group-btn">
                            <button type="submit" class="btn"><i class="icon16 i-search-2 gap-right0"></i></button>
                        </span>
                    </div>
                </form>
                <ul class="nav navbar-nav pull-right">
                    <li class="divider-vertical"></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="icon24 i-cart-2"></i>
                            <span class="notification red">0</span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li role="presentation"><a href="#" class=""><i class="icon16 i-calendar-2"></i>Admin Jenny add event</a></li>
                            <li role="presentation"><a href="#" class=""><i class="icon16 i-file-zip"></i>User Dexter attach file</a></li>
                            <li role="presentation"><a href="#" class=""><i class="icon16 i-stack-picture"></i>User Dexter attach 3 pictures</a></li>
                            <li role="presentation"><a href="#" class=""><i class="icon16 i-cart-add"></i>New orders <span class="notification green">2</span></a></li>
                            <li role="presentation"><a href="#" class=""><i class="icon16 i-bubbles-2"></i>New comments <span class="notification red">5</span></a></li>
                            <li role="presentation"><a href="#" class=""><i class="icon16 i-pie-5"></i>Daily stats is generated</a></li>
                        </ul>
                    </li>
                    <li class="divider-vertical"></li>
                    <li class="dropdown user">
                        <a href="#" class="dropdown-toggle avatar" data-toggle="dropdown">
                            <img src="@ViewContext.CurrentUser().GetAvatar()" alt="@ViewContext.CurrentUser().GetFioSmall()">
                            <span class="more"><i class="icon16 i-arrow-down-2"></i></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li role="presentation"><a href="/account/settings" class=""><i class="icon16 i-cogs"></i>Настройки</a></li>
                            <li role="presentation"><a href="/account/profile" class=""><i class="icon16 i-user"></i>Профиль</a></li>
                            <li role="presentation"><a href="/account/logout" class=""><i class="icon16 i-exit"></i>Выход</a></li>
                        </ul>
                    </li>
                    <li class="divider-vertical"></li>
                </ul>
            </div>
            <!--/.nav-collapse -->
        </nav>
    </header>
    <!-- End #header  -->

    <div class="main">
        <aside id="sidebar">
            <div class="side-options">
                <ul class="list-unstyled">
                    <li>
                        <a href="#" id="collapse-nav" class="act act-primary tip" title="Свернуть меню">
                            <i class="icon16 i-arrow-left-7"></i>
                        </a>
                    </li>
                </ul>
            </div>

            <div class="sidebar-wrapper">
                <nav id="mainnav">
                    <ul class="nav nav-list">
                        <li>
                            <a href="/dashboard">
                                <span class="icon"><i class="icon20 i-dashboard"></i></span>
                                <span class="txt">Сводка</span>
                            </a>
                        </li>
                        <li>
                            <a href="/search">
                                <span class="icon"><i class="icon20 i-search-2"></i></span>
                                <span class="txt">Поиск запчастей</span>
                            </a>
                        </li>
                        <li>
                            <a href="#orders">
                                <span class="icon"><i class="icon20 i-menu-6"></i></span>
                                <span class="txt">Заказы</span>
                            </a>
                            <ul class="sub">
                                <li>
                                    <a href="/orders/current">
                                        <span class="icon"><i class="icon20 i-stack-list"></i></span>
                                        <span class="txt">Текущий заказ</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/orders/active">
                                        <span class="icon"><i class="icon20 i-stack-checkmark"></i></span>
                                        <span class="txt">Активные</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/orders/archive">
                                        <span class="icon"><i class="icon20 i-stack-star"></i></span>
                                        <span class="txt">Выполненные</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="#">
                                <span class="icon"><i class="icon20 i-cogs"></i></span>
                                <span class="txt">Администрирование</span>
                            </a>
                            <ul class="sub">
                                <li>
                                    <a href="/manage/companies">
                                        <span class="icon"><i class="icon20 i-tshirt"></i></span>
                                        <span class="txt">Компании</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/users">
                                        <span class="icon"><i class="icon20 i-users-3"></i></span>
                                        <span class="txt">Пользователи</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/roles">
                                        <span class="icon"><i class="icon20 i-user-5"></i></span>
                                        <span class="txt">Роли</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/orders">
                                        <span class="icon"><i class="icon20 i-list-5"></i></span>
                                        <span class="txt">Заказы</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/support">
                                        <span class="icon"><i class="icon20 i-heart"></i></span>
                                        <span class="txt">Тех. поддержка</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/settings">
                                        <span class="icon"><i class="icon20 i-wrench-3"></i></span>
                                        <span class="txt">Настройки</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/stats">
                                        <span class="icon"><i class="icon20 i-chart"></i></span>
                                        <span class="txt">Статистика</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/manage/audit">
                                        <span class="icon"><i class="icon20 i-pen"></i></span>
                                        <span class="txt">Аудит</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- End #mainnav -->

                @*
                <div class="sidebar-widget center">
                    <h4 class="sidebar-widget-header"><i class="icon i-pie-2"></i> Basic stats</h4>
                    <div class="spark-stats">
                        <ul>
                            <li>
                                <a href="#">
                                    <span class="txt">Visits</span>
                                    <span class="positive">
                                        <span class="up"></span>
                                        <span class="number">20<small>%</small></span>
                                    </span>
                                    <span class="spark">
                                        <span class="positive">5,3,9,6,5,9,7,3,5,2</span>
                                    </span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="txt">Invoices</span>
                                    <span class="negative">
                                        <span class="down"></span>
                                        <span class="number">3<small>%</small></span>
                                    </span>
                                    <span class="spark">
                                        <span class="negative">5,3,9,6,5,9,7,3,5,2</span>
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </div><!-- End .spark-stats -->
                </div><!-- end .sidebar-widget -->

                <div class="sidebar-widget center">
                    <h4 class="sidebar-widget-header"><i class="icon i-fire-2"></i> Site overload</h4>
                    <div id="gauge" style="width:200px; height:150px;"></div>
                    <div id="gauge1" style="width:200px; height:150px;"></div>
                </div><!-- end .sidebar-widget -->*@

            </div>
            <!-- End .sidebar-wrapper  -->
        </aside>
        <!-- End #sidebar  -->

        <section id="content">
            <div class="wrapper">
                <div class="crumb">
                    @Html.Partial("NavigationChain")
                </div>

                <div class="container-fluid">
                    <div id="heading" class="page-header">
                        <h1><i class="icon20 i-dashboard"></i>@ViewBag.Title</h1>
                    </div>

                    @RenderBody()

                </div>
                <!-- End .container-fluid  -->
            </div>
            <!-- End .wrapper  -->
        </section>
    </div>
    <!-- End .main  -->
</body>
</html>
